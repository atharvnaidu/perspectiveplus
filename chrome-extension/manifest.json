{
    "manifest_version": 3,
    "name": "Subtitle Retriever",
    "version": "1.0",
    "description": "Retrieve video timestamps and subtitles",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],  
        "js": ["content.js"]
      }
    ]
  }
  